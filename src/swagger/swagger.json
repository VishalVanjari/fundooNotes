      "/users/register": {
        "post": {
          "summary": "User Registration",
          "operationId": "register user",
          "description": "User is Created",
          "tags": [
            "User"
          ],
          "requestBody": {
            "description": "Enter user details but names, mobile number, email and password are manditory.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "firstName": {
                      "type": "string",
                      "description": "The user's first name"
                    },
                    "lastName": {
                      "type": "string",
                      "description": "The user's last name"
                    },
                    "email": {
                      "type": "string",
                      "description": "The user's email"
                    },
                    "password": {
                      "type": "string",
                      "description": "The user's password"
                    },
                    "mobile": {
                      "type": "string",
                      "description": "The user's mobile number"
                    },
                    "gender": {
                      "type": "string",
                      "description": "The user's gender"
                    },
                    "dob": {
                      "type": "string",
                      "format": "date",
                      "description": "The user's date of birth"
                    }
                  },
                  "required": [
                    "firstName",
                    "lastName",
                    "email",
                    "password",
                    "mobileNo",
                    "gender"
                  ]
                }
              }
            },
            "required": true
          },
          "responses": {
            "201": {
              "description": "User Registred",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "code": {
                        "type": "integer",
                        "example": 201
                      },
                      "data": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer"
                          },
                          "firstName": {
                            "type": "string"
                          },
                          "lastName": {
                            "type": "string"
                          },
                          "email": {
                            "type": "string"
                          },
                          "password": {
                            "type": "string",
                            "description": "Hash of user password"
                          },
                          "mobileNo": {
                            "type": "integer"
                          },
                          "gender": {
                            "type": "string"
                          },
                          "dob": {
                            "type": "string",
                            "format": "date"
                          }
                        }
                      },
                      "message": {
                        "type": "string",
                        "example": "User created successfully"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
